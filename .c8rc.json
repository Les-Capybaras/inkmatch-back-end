{
    "all": true,
    "check-coverage": true,
    "exclude": [
        "bin/**/*.ts",
        "config/**/*.ts",
        "database/**/*.ts",
        "tests/**/*.ts",
        "start/**/*.ts"
    ],
    "reporter": ["text"],
    "report-dir": "coverage",
    "temp-dir": "coverage/.nyc_output",
    "lines": 50,
    "functions": 50,
    "statements": 50
}